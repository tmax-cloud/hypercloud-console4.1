{
  "name": "bridge",
  "description": "CoreOS Tectonic Dashboard",
  "repository": "https://github.com/openshift/console",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "rm -rf ./public/dist/ && ts-node -O '{\"module\":\"commonjs\"}' ./node_modules/.bin/webpack --mode=development --watch --progress",
    "exclude-ts": "rm -f ./node_modules/react-i18next/src/index.d.ts",
    "exclude-ts-linux": "rm -f ./node_modules/react-i18next/src/index.d.ts",
    "build": "rm -rf ./public/dist && cross-env NODE_ENV=production ts-node -O {\"\"\"module\"\"\":\"\"\"commonjs\"\"\"} ./node_modules/webpack/bin/webpack.js --mode=production",
    "build-dev": "rm -rf ./public/dist && cross-env NODE_ENV=development ts-node -O {\"\"\"module\"\"\":\"\"\"commonjs\"\"\"} ./node_modules/webpack/bin/webpack.js --mode=development --watch --progress",
    "build-linux": "rm -rf ./public/dist && cross-env NODE_ENV=production ts-node -O '{\"module\":\"commonjs\"}' ./node_modules/webpack/bin/webpack.js --mode=production",
    "coverage": "jest --coverage .",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --color .",
    "test": "jest",
    "webdriver-update": "webdriver-manager update --versions.chrome=2.38",
    "test-gui-tap": "TAP=true yarn run test-gui",
    "test-gui-openshift": "yarn run test-suite --suite crud --params.openshift true",
    "test-gui": "yarn run test-suite --suite all",
    "postinstall": "patch-package",
    "test-suite": "ts-node -O '{\"module\":\"commonjs\"}' ./node_modules/.bin/protractor integration-tests/protractor.conf.ts",
    "analyze": "NODE_ENV=production ts-node -O '{\"module\":\"commonjs\"}' ./node_modules/.bin/webpack --mode=production --profile --json | awk '{if(NR>2)print}' > public/dist/stats.json && ts-node -O '{\"module\":\"commonjs\"}' ./node_modules/.bin/webpack-bundle-analyzer --mode static -r public/dist/report.html public/dist/stats.json"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transform": {
      ".(ts|tsx|js|jsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es/.*)"
    ],
    "testRegex": "/__tests__/.*\\.(ts|tsx|js|jsx)$",
    "setupFiles": [
      "./__mocks__/requestAnimationFrame.js",
      "./__mocks__/localStorage.ts",
      "./__mocks__/matchMedia.js",
      "./before-tests.js"
    ],
    "coverageDirectory": "__coverage__",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ],
    "collectCoverageFrom": [
      "public/*.{js,jsx,ts,tsx}",
      "public/{components,module,ui}/**/*.{js,jsx,ts,tsx}"
    ]
  },
  "dependencies": {
    "@patternfly/patternfly": "2.71.5",
    "@patternfly/react-core": "3.153.13",
    "@patternfly/react-tokens": "2.8.13",
    "brace": "0.11.x",
    "classnames": "2.x",
